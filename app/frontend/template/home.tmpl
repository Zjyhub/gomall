{{template "header" .}}

<div class="row">
    {{range .Items}}
    <div class="card border-0 col-lg-4 col-md-6 col-sm-12 p-5 col-xl-3">
        <img src="{{ .Picture}}" class="card-img-top" alt="...">
        <div class="card-body">
            <p class="card-text">{{ .Name}}</p>
            <h5 class="card-title">{{ .Price}}</h5>
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>
    {{end}}
</div>

{{template "footer" .}}